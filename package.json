    "url": "https://github.com/blitz-js/superjson"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.7.18",
    "benchmark": "^2.1.4",
    "decimal.js": "^10.4.2",
    "eslint-plugin-es5": "^1.5.0",
    "husky": "^8.0.1",
    "mongodb": "^4.10.0",
    "publish-please": "^5.5.2",
    "tsdx": "^0.14.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "copy-anything": "^3.0.2"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.11.1",
    "**/@typescript-eslint/parser": "^4.11.1",
    "**/jest": "^26.6.3",
    "**/ts-jest": "^26.4.4"
